{
  "name": "wiz-battles",
  "version": "1.0.0",
  "description": "my game",
  "main": "index.js",
  "scripts": {
    "build": "set BABEL_ENV=ssr&&set NODE_ENV=prodSSR&&webpack -p --progress",
    "prestart": "npm run build",
    "start": "set BABEL_ENV=ssr&&set NODE_ENV=prodSSR&&npx babel-node ./server/index-server.js",
    "prodCR": "set NODE_ENV=prodCR&&webpack -p --progress&&npx babel-node ./server/index-server.js",
    "dev": "set BABEL_ENV=ssr&&set NODE_ENV=devSSR&&nodemon --exec babel-node ./server/dev-server.js",
    "devCR": "set NODE_ENV=devCR&&npx babel-node ./server/dev-server.js",
    "lint": "set NODE_ENV=lint&&npx babel-node ./server/dev-server.js"
  },
  "nodemonConfig": {
    "watch": [
      "server/",
      "config/",
      "webpack.config.js",
      "package.json"
    ],
    "delay": "2500"
  },
  "engines": {
    "node": "8.9.4"
  },
  "keywords": [
    "react",
    "react-redux",
    "konva",
    "react-konva"
  ],
  "author": "evgmsk",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.8.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "latest",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "latest",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "open": "^6.1.0",
    "postcss-flexbugs-fixes": "latest",
    "postcss-loader": "^3.0.0",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "latest",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-dev-server": "latest",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@loadable/component": "^5.7.0",
    "@loadable/server": "^5.8.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "latest",
    "config": "^3.0.1",
    "cookie-parser": "latest",
    "express": "latest",
    "express-validator": "^5.3.1",
    "firebase-admin": "^7.2.0",
    "helmet": "^3.16.0",
    "ignore-styles": "^5.0.1",
    "konva": "latest",
    "mongodb": "^3.2.2",
    "mongoose": "^5.4.22",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.0",
    "react-icons": "^3.5.0",
    "react-konva": "latest",
    "react-redux": "latest",
    "react-router": "latest",
